Program(
	MainObject(
		Identifier(
			MyMain
		),
		List(
			Println(
				StringLit(
					Hello World
				)
			), 
			While(
				True(),
				Block(
					List(
						Assign(
							Identifier(
								syntax
							),
							Plus(
								IntLit(2),
								Plus(
									IntLit(2),
									IntLit(2)
								)
							)
						), 
						Assign(
							Identifier(myInt),
							Div(
								IntLit(2),
								IntLit(2)
							)
						)
					)
				)
			)
		)
	),
	List()
)
